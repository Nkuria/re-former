<%= form_for @user, url: {action: "update"}, html: {class: "form1"} do |f| %>
  <%= f.label :username, "Username" %>
  <%= f.text_field :username, placeholder: "Enter Username" %>
  <%= f.label :email, "Email" %>
  <%= f.email_field :email, placeholder: "example@example.com"  %>
  <%= f.label :password, "Password" %>
  <%= f.password_field :password %>
  <%= f.submit "Update" %>
  <% end %>
